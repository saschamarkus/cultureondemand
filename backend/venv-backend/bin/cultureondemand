#!/home/karl/Code/cultureondemand/backend/venv-backend/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cultureondemand','console_scripts','cultureondemand'
__requires__ = 'cultureondemand'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cultureondemand', 'console_scripts', 'cultureondemand')()
    )
